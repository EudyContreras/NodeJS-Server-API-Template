  "homepage": "https://github.com/EudyContreras/NodeJS-Server-API-Template#readme",
  "description": "A NodeJS template API and Server meant to speed up the initial development process.",
  "repository": {
    "type": "git",
    "url": "https://github.com/EudyContreras/NodeJS-Server-API-Template"
  },
  "proxy": "https://localhost:8080/",
  "main": "build/server.js",
  "scripts": {
    "build:ts": "tsc -p .",
    "build:server": "webpack --config webpack/webpack.server.config.js",
    "build:client": "webpack --config webpack/webpack.client.config.js",
    "build:worker": "webpack --config webpack/webpack.worker.config.js",
    "build:watch:server": "webpack --config webpack/webpack.server.config.js/ --watch",
    "build:watch:client": "webpack --config webpack/webpack.client.config.js/ --watch",
    "build:dev": "npm run build:watch:client && npm run build:watch:server",
    "build:prod": "npm run build:client && npm run build:server && npm run build:worker",
    "build": "npm run build:prod",
    "build:dev:start": "concurrently \"npm run build:dev && NODE_ENV=development\" \"nodemon --delay 2500ms build/server.js\"",
    "build:prod:start": "npm run build:prod && npm run start",
    "start:dev": "nodemon ./build/server.js",
    "start": "node build/server.js"
  },
  "keywords": [
    "ssr",
    "csr",
    "PWA",
    "Pogressive Web Application",
    "typescript",
    "TS React",
    "api-template",
    "isomorphic",
    "express",
    "node",
    "react"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
