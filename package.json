  "description": "A NodeJS template API and Server meant to speed up the initial development process.",
  "main": "src/server.ts",
  "scripts": {
    "install": "npm install --prefix start",
    "build:ts": "tsc -p .",
    "build:server": "webpack",
    "build:client": "webpack --config webpack.client.config.js",
    "build": "npm run build:ts & npm run build:server & npm run build:client",
    "serve": "node dist/server.js",
    "start": "npm run build && npm run serve",
    "dev": "nodemon src/server.ts"
  },
