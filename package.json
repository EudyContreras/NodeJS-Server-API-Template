  "homepage": "https://github.com/EudyContreras/NodeJS-Server-API-Template#readme",
  "description": "A NodeJS template API and Server meant to speed up the initial development process.",
  "repository": {
    "type": "git",
    "url": "https://github.com/EudyContreras/NodeJS-Server-API-Template"
  },
  "proxy": "https://localhost:8080/",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "cleanup": "rimraf pre dist build",
    "lint-and-check": "eslint 'src/**/*.{js,jsx,ts,tsx}' && echo 'ESLint done checking!'",
    "lint-and-fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix . && echo 'ESLint done checking!.'",
    "fix-code": "prettier-eslint --write $PWD/'src/**/*.{js,jsx,ts,tsx}' ",
    "fix-styles": "prettier-stylelint --write $PWD/'src/**/*.{css,scss,less,sass}' ",
    "fix-all": "npm-run-all fix-code fix-styles",
    "compile-ts": "npm run cleanup && tsc -b ./ && cpy --cwd=src --parents '**/*' '!**/*.(ts|js|tsx|jsx)' ../dist/ ",
    "build:client": "webpack --config webpack/configs/webpack.config.client.js",
    "build:client:watch": "webpack --config webpack/configs/webpack.config.client.js --watch",
    "build:server:watch": "webpack --config webpack/configs/webpack.config.server.js --watch",
    "build:prod": "npm run cleanup && cross-env NODE_ENV=production PRECOMPILE=true REACT_HMR=false && npm run compile-ts && webpack",
    "build:dev": "npm run cleanup && cross-env NODE_ENV=development PRECOMPILE=false REACT_HMR=false webpack",
    "build:prod:start": "npm run build:prod && npm run start",
    "build:dev:start": "npm run build:dev && npm run start",
    "debug:hot": "npm run cleanup && cross-env NODE_ENV=development REACT_HMR=true PRECOMPILE=false CSR=false concurrently --kill-others \"npm-run-all compile-ts build:client build:server:watch\" \"webpack-dev-server --hot\"",
    "debug:hot:server": "npm run cleanup && cross-env NODE_ENV=development REACT_HMR=false PRECOMPILE=false CSR=true npm run build:server:watch",
    "debug:hot:client": "npm run cleanup && cross-env NODE_ENV=development REACT_HMR=true PRECOMPILE=false CSR=true npm-run-all build:client && cross-env NODE_ENV=development CSR=true REACT_HMR=true PRECOMPILE=false webpack-dev-server --hot"
  },
  "keywords": [
    "ssr",
    "csr",
    "PWA",
    "Pogressive Web Application",
    "typescript",
    "TS React",
    "api-template",
    "isomorphic",
    "express",
    "node",
    "react"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
